{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": "airbnb",
  "rules": {
    "import/no-named-as-default": 0,
    "import/no-extraneous-dependencies": [
      { 
        "devDependencies": true
      }
    ],
    "react/prop-types": 0,
    "no-unused-expressions": 0,
    "jsx-a11y/label-has-for": 0,
    "prefer-template": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  }
}