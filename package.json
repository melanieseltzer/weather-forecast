{
  "name": "weather-forecast",
  "version": "1.0.0",
  "description": "Search for a city and get a 24 hour forecast.",
  "main": "index.js",
  "repository": "git@github.com:melanieseltzer/weather-forecast.git",
  "homepage": "https://melanieseltzer.github.io/weather-forecast",
  "scripts": {
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "npm run lint && npm run test && NODE_ENV=development webpack-dev-server --mode development --hot --inline --open",
    "build": "npm run lint && npm run test && NODE_ENV=production webpack --mode production",
    "prod": "NODE_ENV=production node server.js",
    "postinstall": "npm run build"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupTestFrameworkScriptFile": "./setupTests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "author": "Melanie Seltzer <melleh11@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "moxios": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@data-ui/sparkline": "0.0.54",
