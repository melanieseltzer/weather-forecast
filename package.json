    "moment-timezone": "^0.5.21",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "5.0.7",
    "redux": "^4.0.0",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.8",
    "styled-components": "^3.3.3",
